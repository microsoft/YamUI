{
  "name": "yamui",
  "description": "UI component framework for Yammer.com",
  "version": "0.0.6",
  "style": "dist/yamui-base.css",
  "license": "MIT",
  "dependencies": {
    "classnames": "=2.2.5",
    "normalize.css": "=6.0.0",
    "office-ui-fabric-react": "=4.19.0",
    "typescript-string-enums": "=0.3.2"
  },
  "devDependencies": {
    "@storybook/addon-options": "=3.1.2",
    "@storybook/react": "=3.1.3",
    "@types/classnames": "=0.0.32",
    "@types/enzyme": "=2.8.0",
    "@types/jest": "=20.0.1",
    "@types/node": "=7.0.12",
    "@types/prop-types": "=15.5.1",
    "@types/react": "=15.0.27",
    "@types/react-dom": "=15.5.0",
    "@types/storybook__react": "=3.0.0",
    "autoprefixer": "=6.5.1",
    "backstopjs": "=2.6.14",
    "chalk": "=1.1.3",
    "concurrently": "=3.4.0",
    "cpx": "=1.5.0",
    "css-loader": "=0.27.3",
    "enzyme": "=2.8.2",
    "eslint": "=3.19.0",
    "eslint-config-airbnb": "=14.1.0",
    "eslint-plugin-header": "=1.0.0",
    "eslint-plugin-import": "=2.2.0",
    "eslint-plugin-jsx-a11y": "=4.0.0",
    "eslint-plugin-react": "=6.10.3",
    "file-loader": "=0.11.1",
    "glob": "=7.1.1",
    "identity-obj-proxy": "=3.0.0",
    "jest": "=20.0.4",
    "jest-serializer-enzyme": "=1.0.0",
    "npm-run-all": "=4.0.2",
    "phantomjs-prebuilt": "=2.1.14",
    "postcss": "=5.2.16",
    "postcss-cli": "=3.2.0",
    "postcss-import": "=9.1.0",
    "postcss-loader": "=1.3.3",
    "postcss-scss": "=0.4.1",
    "precss": "=1.4.0",
    "raw-loader": "=0.5.1",
    "react": "=15.5.4",
    "react-addons-test-utils": "=15.5.1",
    "react-dom": "=15.5.4",
    "react-test-renderer": "=15.5.4",
    "rimraf": "=2.6.1",
    "serve": "=5.1.4",
    "storybook-readme": "=3.0.4",
    "style-loader": "=0.16.1",
    "svg-sprite-loader": "=3.0.5",
    "svgo-loader": "=1.2.1",
    "ts-jest": "=20.0.6",
    "ts-loader": "=2.0.3",
    "tslint": "=5.2.0",
    "tslint-config-airbnb": "=1.1.1",
    "typescript": "=2.3.4"
  },
  "peerDependencies": {
    "react": "=15.5.4",
    "react-dom": "=15.5.4"
  },
  "scripts": {
    "clean:dist": "rimraf dist/*",
    "clean:docs": "rimraf docs/*",
    "clean:build": "rimraf build/*",
    "copy:story-assets": "cpx \"src/stories/assets/**/*\" \"dist/stories/assets\"",
    "copy:icon-assets": "cpx \"src/components/Icon/icons/*\" \"dist/components/Icon/icons\"",
    "copy:readmes": "cpx \"src/components/**/README.md\" \"dist/components\"",
    "build:css:global": "postcss src/css/index.css --output dist/yamui-base.css",
    "build:css:components": "postcss \"src/components/**/*.css\" --dir \"dist/components\" --base \"src/components\"",
    "build:ts:components": "tsc --project .",
    "build": "run-s clean:dist clean:build copy:** build:**",
    "test:eslint:root": "eslint \"./*.+(js|jsx)\"",
    "test:eslint:sources": "eslint \"+(src|config)/**/*.+(js|jsx)\"",
    "test:eslint": "run-p test:eslint:*",
    "test:tslint": "tslint \"src/**/*.+(ts|tsx)\"",
    "test:unit": "jest --config config/jest/config.json",
    "test:visual:build": "run-s build storybook:static:build",
    "test:visual:serve": "run-p storybook:static:serve test:visual:test-and-stop",
    "test:visual:test-and-stop": "run-s --continue-on-error visual:test storybook:static:stop",
    "test:visual": "run-s test:visual:build test:visual:serve",
    "test": "run-s build test:*",
    "watch:css:global": "postcss src/css/index.css --output dist/yamui-base.css --watch",
    "watch:css:components": "postcss \"src/components/**/*.css\" --dir \"dist/components\" --base \"src/components\" --watch",
    "watch:ts:components": "tsc --project . --watch",
    "watch": "run-p watch:**",
    "start:dev": "yarn build && run-p watch storybook:start",
    "start:test": "jest --config config/jest/config.json --watchAll",
    "report:unit": "open \"build/jest/coverage/index.html\"",
    "report:visual": "backstop openReport --config config/backstop/config.js",
    "report": "run-p report:*",
    "storybook:start": "start-storybook --port 5555 --config-dir config/storybook",
    "storybook:static:build": "yarn clean:docs && build-storybook --config-dir config/storybook --output-dir docs",
    "storybook:static:serve": "serve docs --port 5555",
    "storybook:static:stop": "pkill -f \"sh -c serve docs --port 5555\"",
    "visual:test": "backstop test --config config/backstop/config.js",
    "visual:approve": "backstop approve --config config/backstop/config.js",
    "prepublishOnly": "yarn build"
  }
}
